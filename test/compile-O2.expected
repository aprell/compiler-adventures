#include <stdio.h>

int main(void) {
    int w = 0;
    int x = 0;
    int y = 0;
    int z = 0;

    scanf("%d", &x);
    scanf("%d", &y);
    x = x * 3;
    x = x == y;

    printf("w: %3d\n", w);
    printf("x: %3d\n", x);
    printf("y: %3d\n", y);
    printf("z: %3d\n", z);

    return 0;
}

